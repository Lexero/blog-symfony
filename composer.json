{
    "name": "alex/blog-symfony",
    "description": "pet project for pdp",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cocur/slugify": "^4.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "fakerphp/faker": "^1.21",
        "friendsofsymfony/rest-bundle": "^3.4",
        "lexik/jwt-authentication-bundle": "^2.16",
        "ramsey/uuid-doctrine": "^1.8",
        "sonata-project/admin-bundle": "^4.21",
        "sonata-project/doctrine-orm-admin-bundle": "^4.8",
        "symfony/amqp-messenger": "6.2.*",
        "symfony/cache": "^6.2",
        "symfony/config": "^6.1",
        "symfony/console": "6.0.*",
        "symfony/dependency-injection": "^6.2",
        "symfony/doctrine-bridge": "^6.2",
        "symfony/dotenv": "6.0.*",
        "symfony/error-handler": "^6.1",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.2",
        "symfony/http-client": "6.0.*",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/mailer": "6.0.*",
        "symfony/mailgun-mailer": "6.0.*",
        "symfony/messenger": "6.2.*",
        "symfony/mime": "^6.2",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "^6.2",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "^6.1",
        "symfony/twig-bundle": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/var-exporter": "^6.2",
        "symfony/yaml": "6.0.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/maker-bundle": "^1.48",
        "symfony/stopwatch": "6.0.*",
        "symfony/var-dumper": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*"
    }
}
